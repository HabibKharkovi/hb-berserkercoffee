        <div class="hb-content">
          {%- if section.settings.title != blank -%}
            <div class="hb-text">
              {%- if section.settings.title != blank -%}
                <h2 class="hb-h2">{{ section.settings.title }}</h2>
              {%- endif -%}
              {%- if section.settings.text != blank -%}
                <p class="hb-p hb_small_p">{{ section.settings.text }}</p>
              {%- endif -%}
            </div>
          {%- endif -%}
          {%- form 'customer', id: newsletter_id, class: 'input-form' -%}
            <div class="hb-input hb_center">
              <div class="hb-input-wrapper">
                <input type="email" name="contact[email]" placeholder="E-mail" required>
              </div>
              <div class="hb-button">
                <button type="submit" class="hb-btn {{ section.settings.btn_type }}">
                  {{ section.settings.btn_label }}
                </button>
              </div>
            </div>
            {%- if form.posted_successfully? -%}
              <div class="form__banner banner banner--success">
                <span class="banner__ribbon">{% render 'icon' with 'form-success' %}</span>
                <p class="banner__content">{{ 'general.newsletter.success' | t }}</p>
              </div>
            {%- else -%}
              {%- if form.errors -%}
                <div class="form__banner banner banner--error">
                  <span class="banner__ribbon">{% render 'icon' with 'form-error' %}</span>
                  <p class="banner__content">{{ form.errors.messages.email }}</p>
                </div>
              {%- endif -%}
            {%- endif -%}
          {% endform %}
        </div>
      </div>
    </div>
  </div>
</div>
